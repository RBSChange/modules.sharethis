<?xml version="1.0" encoding="UTF-8"?>
<blocks>
	<block type="modules_sharethis_sharepage" icon="sharethis" afterAll="true" cache="true" cache-deps="[modules_sharethis/link]">
		<parameters>
			<parameter name="groupCode" type="String" default-value="currentpage" />
		</parameters>
	</block>
	<block type="modules_sharethis_Shareurl" icon="sharethis" cache="true" cache-deps="[modules_sharethis/link]"
		label="&amp;modules.sharethis.bo.blocks.shareurl.block-title;">
		<parameters>
			<parameter name="groupCode" type="String" default-value="default" />
			<parameter name="url" type="String" min-occurs="1" />
			<parameter name="title" type="String" min-occurs="1" />
		</parameters>
	</block>
	<block type="modules_sharethis_Pinit" icon="sharethis">
		<parameters>
			<parameter name="countlayout" type="String" list-id="modules_sharethis/pinitcountlayout" default-value="horizontal" />
		</parameters>
	</block>
	<block type="modules_sharethis_Googleplus" icon="sharethis">
		<parameters>
			<parameter name="annotation" type="String" list-id="modules_sharethis/googleplusannotation" default-value="bubble" />
			<parameter name="size" type="String" list-id="modules_sharethis/googleplussize" default-value="standard" />
		</parameters>
	</block>
	<block type="modules_sharethis_Facebooklike" icon="sharethis">
		<parameters>
			<parameter name="appId" type="String" default-value="" />
			<parameter name="ref" type="String" default-value="" />
			<parameter name="send" type="Boolean" default-value="true" />
			<parameter name="showfaces" type="Boolean" default-value="true" />
			<parameter name="blockwidth" type="Integer" default-value="450" />
			<parameter name="verb" type="String" list-id="modules_sharethis/facebooklikeverb" default-value="like" />
			<parameter name="colorscheme" type="String" list-id="modules_sharethis/facebooklikecolorscheme" default-value="light" />
			<parameter name="font" type="String" list-id="modules_sharethis/facebooklikefont" default-value="lucida grande" />
			<parameter name="layout" type="String" list-id="modules_sharethis/facebooklikelayout" default-value="standard" />
		</parameters>
	</block>
</blocks>