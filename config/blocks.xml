		afterAll="true" cache="true" cache-deps="[modules_sharethis/link]">
		<parameters>
			<parameter name="groupCode" type="String" default-value="currentpage" />
		</parameters>
	</block>
	<block type="modules_sharethis_Shareurl" icon="sharethis" label="&amp;modules.sharethis.bo.blocks.shareurl.Title;"
